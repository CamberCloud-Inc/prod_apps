{
  "name": "tfactivity-tf-regulon-analysis",
  "title": "TF Activity",
  "description": " Regulon Analysis",
  "version": "1.0.0",
  "engineType": "NEXTFLOW",
  "command": "nextflow run nf-core/tfactivity --input ${input} --outdir ${outdir} -r  Transcription factor activity inference:1.0.0 ",
