{
  "name": "clipseq-rna-binding-proteins",
  "title": "CLIPseq",
  "description": " RNA-Protein Interaction Mapping",
  "version": "1.0.0",
  "engineType": "NEXTFLOW",
  "command": "nextflow run nf-core/clipseq --input ${input} --outdir ${outdir} -r CLIP-seq analysis for RNA-binding protein targets.:1.0.4 ",
