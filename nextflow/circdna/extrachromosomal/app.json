{
  "name": "circdna-ecdna-detection",
  "title": "CircDNA",
  "description": " Extrachromosomal DNA Detection",
  "version": "1.0.0",
  "engineType": "NEXTFLOW",
  "command": "nextflow run nf-core/circdna --input ${input} --outdir ${outdir} -r Detect extrachromosomal DNA (ecDNA) and circular DNA in cancer.:1.0.7 ",
  "imageUrl": "https://raw.githubusercontent.com/nf-core/circdna/master/docs/images/nf-core-circdna_logo_light.png",
  "content": "<h2><strong>CircDNA</strong></h2><p> Extrachromosomal DNA Detection</p>",
  "spec": [
    {"type": "Stash File", "label": "Input", "name": "input", "required": true, "restrictions": {"allow_files": true, "allowed_file_types": [".csv"]}},
    {"type": "Stash File", "label": "Output", "name": "outdir", "defaultValue": "./circdna-results", "required": true, "restrictions": {"allow_files": false, "allow_folders": true}}
  ],
  "jobConfig": [{"type": "Select", "label": "Resources", "name": "system_size", "options": [{"label": "LARGE", "value": "large", "mapValue": {"nodeSize": "LARGE", "numNodes": 1, "withGpu": false}}], "defaultValue": "large"}],
  "tags": [{"name": "circdna", "type": "task"}]
}
