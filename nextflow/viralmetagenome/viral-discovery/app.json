{
  "name": "viralmetagenome-viral-discovery",
  "title": "Viral Metagenome",
  "description": " Viral Discovery",
  "version": "1.0.0",
  "engineType": "NEXTFLOW",
  "command": "nextflow run nf-core/viralmetagenome --input ${input} --outdir ${outdir} -r  Environmental virome characterization:1.0.0 ",
  "imageUrl": "https://raw.githubusercontent.com/nf-core/viralmetagenome/master/docs/images/nf-core-viralmetagenome_logo_light.png",
  "content": "<h2><strong>Viral Metagenome</strong></h2><p> Viral Discovery</p>",
  "spec": [
    {"type": "Stash File", "label": "Input", "name": "input", "required": true, "restrictions": {"allow_files": true, "allowed_file_types": [".csv", ".tsv"]}},
    {"type": "Stash File", "label": "Output", "name": "outdir", "defaultValue": "./viralmetagenome-results", "required": true, "restrictions": {"allow_files": false, "allow_folders": true}}
  ],
  "jobConfig": [{"type": "Select", "label": "Resources", "name": "system_size", "options": [{"label": "LARGE", "value": "large", "mapValue": {"nodeSize": "LARGE", "numNodes": 1, "withGpu": false}}], "defaultValue": "large"}],
  "tags": [{"name": "viralmetagenome", "type": "task"}]
}
