{
  "name": "molkart-spatial-multiomics",
  "title": "Molkart",
  "description": " Spatial Multi-omics",
  "version": "1.0.0",
  "engineType": "NEXTFLOW",
  "command": "nextflow run nf-core/molkart --input ${input} --outdir ${outdir} -r  Tissue architecture and protein neighborhoods:1.0.0 ",
  "imageUrl": "https://raw.githubusercontent.com/nf-core/molkart/master/docs/images/nf-core-molkart_logo_light.png",
  "content": "<h2><strong>Molkart</strong></h2><p> Spatial Multi-omics</p>",
  "spec": [
    {"type": "Stash File", "label": "Input", "name": "input", "required": true, "restrictions": {"allow_files": true, "allowed_file_types": [".csv", ".tsv"]}},
    {"type": "Stash File", "label": "Output", "name": "outdir", "defaultValue": "./molkart-results", "required": true, "restrictions": {"allow_files": false, "allow_folders": true}}
  ],
  "jobConfig": [{"type": "Select", "label": "Resources", "name": "system_size", "options": [{"label": "LARGE", "value": "large", "mapValue": {"nodeSize": "LARGE", "numNodes": 1, "withGpu": false}}], "defaultValue": "large"}],
  "tags": [{"name": "molkart", "type": "task"}]
}
