{
  "name": "ribomsqc-ribo-profiling-qc",
  "title": "RiboMS QC",
  "description": " Ribosomal Profiling QC",
  "version": "1.0.0",
  "engineType": "NEXTFLOW",
  "command": "nextflow run nf-core/ribomsqc --input ${input} --outdir ${outdir} -r  Ribosome profiling quality control:1.0.0 ",
  "imageUrl": "https://raw.githubusercontent.com/nf-core/ribomsqc/master/docs/images/nf-core-ribomsqc_logo_light.png",
  "content": "<h2><strong>RiboMS QC</strong></h2><p> Ribosomal Profiling QC</p>",
  "spec": [
    {"type": "Stash File", "label": "Input", "name": "input", "required": true, "restrictions": {"allow_files": true, "allowed_file_types": [".csv", ".tsv"]}},
    {"type": "Stash File", "label": "Output", "name": "outdir", "defaultValue": "./ribomsqc-results", "required": true, "restrictions": {"allow_files": false, "allow_folders": true}}
  ],
  "jobConfig": [{"type": "Select", "label": "Resources", "name": "system_size", "options": [{"label": "LARGE", "value": "large", "mapValue": {"nodeSize": "LARGE", "numNodes": 1, "withGpu": false}}], "defaultValue": "large"}],
  "tags": [{"name": "ribomsqc", "type": "task"}]
}
