{
  "name": "lncpipe-lncrna-discovery",
  "title": "lncPipe",
  "description": " Long Non-coding RNA",
  "version": "1.0.0",
  "engineType": "NEXTFLOW",
  "command": "nextflow run nf-core/lncpipe --input ${input} --outdir ${outdir} -r  lncRNA discovery and annotation:1.0.0 ",
  "imageUrl": "https://raw.githubusercontent.com/nf-core/lncpipe/master/docs/images/nf-core-lncpipe_logo_light.png",
  "content": "<h2><strong>lncPipe</strong></h2><p> Long Non-coding RNA</p>",
  "spec": [
    {"type": "Stash File", "label": "Input", "name": "input", "required": true, "restrictions": {"allow_files": true, "allowed_file_types": [".csv", ".tsv"]}},
    {"type": "Stash File", "label": "Output", "name": "outdir", "defaultValue": "./lncpipe-results", "required": true, "restrictions": {"allow_files": false, "allow_folders": true}}
  ],
  "jobConfig": [{"type": "Select", "label": "Resources", "name": "system_size", "options": [{"label": "LARGE", "value": "large", "mapValue": {"nodeSize": "LARGE", "numNodes": 1, "withGpu": false}}], "defaultValue": "large"}],
  "tags": [{"name": "lncpipe", "type": "task"}]
}
