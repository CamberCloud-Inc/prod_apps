{
  "name": "genomeskim-organelle-genomes",
  "title": "Genome Skim",
  "description": " Organelle Genomes",
  "version": "1.0.0",
  "engineType": "NEXTFLOW",
  "command": "nextflow run nf-core/genomeskim --input ${input} --outdir ${outdir} -r  Chloroplast and mitochondrial assembly:1.0.0 ",
  "imageUrl": "https://raw.githubusercontent.com/nf-core/genomeskim/master/docs/images/nf-core-genomeskim_logo_light.png",
  "content": "<h2><strong>Genome Skim</strong></h2><p> Organelle Genomes</p>",
  "spec": [
    {"type": "Stash File", "label": "Input", "name": "input", "required": true, "restrictions": {"allow_files": true, "allowed_file_types": [".csv", ".tsv"]}},
    {"type": "Stash File", "label": "Output", "name": "outdir", "defaultValue": "./genomeskim-results", "required": true, "restrictions": {"allow_files": false, "allow_folders": true}}
  ],
  "jobConfig": [{"type": "Select", "label": "Resources", "name": "system_size", "options": [{"label": "LARGE", "value": "large", "mapValue": {"nodeSize": "LARGE", "numNodes": 1, "withGpu": false}}], "defaultValue": "large"}],
  "tags": [{"name": "genomeskim", "type": "task"}]
}
