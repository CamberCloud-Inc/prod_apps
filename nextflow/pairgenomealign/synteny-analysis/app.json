{
  "name": "pairgenomealign-synteny-analysis",
  "title": "Pairwise Genome Alignment",
  "description": " Synteny Analysis",
  "version": "1.0.0",
  "engineType": "NEXTFLOW",
  "command": "nextflow run nf-core/pairgenomealign --input ${input} --outdir ${outdir} -r  Comparative genome alignment:1.0.0 ",
  "imageUrl": "https://raw.githubusercontent.com/nf-core/pairgenomealign/master/docs/images/nf-core-pairgenomealign_logo_light.png",
  "content": "<h2><strong>Pairwise Genome Alignment</strong></h2><p> Synteny Analysis</p>",
  "spec": [
    {"type": "Stash File", "label": "Input", "name": "input", "required": true, "restrictions": {"allow_files": true, "allowed_file_types": [".csv", ".tsv"]}},
    {"type": "Stash File", "label": "Output", "name": "outdir", "defaultValue": "./pairgenomealign-results", "required": true, "restrictions": {"allow_files": false, "allow_folders": true}}
  ],
  "jobConfig": [{"type": "Select", "label": "Resources", "name": "system_size", "options": [{"label": "LARGE", "value": "large", "mapValue": {"nodeSize": "LARGE", "numNodes": 1, "withGpu": false}}], "defaultValue": "large"}],
  "tags": [{"name": "pairgenomealign", "type": "task"}]
}
