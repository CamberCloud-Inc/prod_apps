{
  "name": "grommac3s",
  "description": "MD sim basics",
  "content": "<p class=\"text-node\">It simulates a carbon nanotube structure using the \"unbreakable\" simulation setup, which demonstrates molecular dynamics of a complex carbon structure.</p>",
  "imageUrl": "",
  "command": "git clone https://github.com/CamberCloud-Inc/prod_apps.git && cd prod_apps && sh ./run_sim.sh $temp\r\n$",
  "engineType": "LAMMPS",
  "jobConfig": [
    {
      "type": "Select",
      "label": "System Size",
      "name": "system_size",
      "options": [
        {
          "label": "Default",
          "value": "default",
          "mapValue": {
            "nodeSize": "XSMALL",
            "numNodes": 1,
            "withGpu": false
          }
        }
      ],
      "defaultValue": "default"
    }
  ],
  "spec": [
    {
      "type": "Input",
      "label": "temp",
      "name": "temp"
    }
  ]
}