{
  "name": "genomeannotator-eukaryote-annotation",
  "title": "Genome Annotator",
  "description": " Eukaryotic Annotation",
  "version": "1.0.0",
  "engineType": "NEXTFLOW",
  "command": "nextflow run nf-core/genomeannotator --input ${input} --outdir ${outdir} -r  Eukaryotic genome annotation:1.0.0 ",
  "imageUrl": "https://raw.githubusercontent.com/nf-core/genomeannotator/master/docs/images/nf-core-genomeannotator_logo_light.png",
  "content": "<h2><strong>Genome Annotator</strong></h2><p> Eukaryotic Annotation</p>",
  "spec": [
    {"type": "Stash File", "label": "Input", "name": "input", "required": true, "restrictions": {"allow_files": true, "allowed_file_types": [".csv", ".tsv"]}},
    {"type": "Stash File", "label": "Output", "name": "outdir", "defaultValue": "./genomeannotator-results", "required": true, "restrictions": {"allow_files": false, "allow_folders": true}}
  ],
  "jobConfig": [{"type": "Select", "label": "Resources", "name": "system_size", "options": [{"label": "LARGE", "value": "large", "mapValue": {"nodeSize": "LARGE", "numNodes": 1, "withGpu": false}}], "defaultValue": "large"}],
  "tags": [{"name": "genomeannotator", "type": "task"}]
}
