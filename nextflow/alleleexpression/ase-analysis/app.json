{
  "name": "alleleexpression-ase-analysis",
  "title": "Allele Expression",
  "description": " ASE Analysis",
  "version": "1.0.0",
  "engineType": "NEXTFLOW",
  "command": "nextflow run nf-core/alleleexpression --input ${input} --outdir ${outdir} -r  Allele-specific expression quantification:1.0.0 ",
  "imageUrl": "https://raw.githubusercontent.com/nf-core/alleleexpression/master/docs/images/nf-core-alleleexpression_logo_light.png",
  "content": "<h2><strong>Allele Expression</strong></h2><p> ASE Analysis</p>",
  "spec": [
    {"type": "Stash File", "label": "Input", "name": "input", "required": true, "restrictions": {"allow_files": true, "allowed_file_types": [".csv", ".tsv"]}},
    {"type": "Stash File", "label": "Output", "name": "outdir", "defaultValue": "./alleleexpression-results", "required": true, "restrictions": {"allow_files": false, "allow_folders": true}}
  ],
  "jobConfig": [{"type": "Select", "label": "Resources", "name": "system_size", "options": [{"label": "LARGE", "value": "large", "mapValue": {"nodeSize": "LARGE", "numNodes": 1, "withGpu": false}}], "defaultValue": "large"}],
  "tags": [{"name": "alleleexpression", "type": "task"}]
}
