{
  "name": "rnadnavar-rna-editing-analysis",
  "title": "RNA-DNA Variant",
  "description": " RNA Editing",
  "version": "1.0.0",
  "engineType": "NEXTFLOW",
  "command": "nextflow run nf-core/rnadnavar --input ${input} --outdir ${outdir} -r  Combined RNA and DNA variant calling:1.0.0 ",
  "imageUrl": "https://raw.githubusercontent.com/nf-core/rnadnavar/master/docs/images/nf-core-rnadnavar_logo_light.png",
  "content": "<h2><strong>RNA-DNA Variant</strong></h2><p> RNA Editing</p>",
  "spec": [
    {"type": "Stash File", "label": "Input", "name": "input", "required": true, "restrictions": {"allow_files": true, "allowed_file_types": [".csv", ".tsv"]}},
    {"type": "Stash File", "label": "Output", "name": "outdir", "defaultValue": "./rnadnavar-results", "required": true, "restrictions": {"allow_files": false, "allow_folders": true}}
  ],
  "jobConfig": [{"type": "Select", "label": "Resources", "name": "system_size", "options": [{"label": "LARGE", "value": "large", "mapValue": {"nodeSize": "LARGE", "numNodes": 1, "withGpu": false}}], "defaultValue": "large"}],
  "tags": [{"name": "rnadnavar", "type": "task"}]
}
