{
  "name": "lsmquant-label-free-ms",
  "title": "LSM Quant",
  "description": " Label-free MS",
  "version": "1.0.0",
  "engineType": "NEXTFLOW",
  "command": "nextflow run nf-core/lsmquant --input ${input} --outdir ${outdir} -r  Label-free mass spec quantification:1.0.0 ",
  "imageUrl": "https://raw.githubusercontent.com/nf-core/lsmquant/master/docs/images/nf-core-lsmquant_logo_light.png",
  "content": "<h2><strong>LSM Quant</strong></h2><p> Label-free MS</p>",
  "spec": [
    {"type": "Stash File", "label": "Input", "name": "input", "required": true, "restrictions": {"allow_files": true, "allowed_file_types": [".csv", ".tsv"]}},
    {"type": "Stash File", "label": "Output", "name": "outdir", "defaultValue": "./lsmquant-results", "required": true, "restrictions": {"allow_files": false, "allow_folders": true}}
  ],
  "jobConfig": [{"type": "Select", "label": "Resources", "name": "system_size", "options": [{"label": "LARGE", "value": "large", "mapValue": {"nodeSize": "LARGE", "numNodes": 1, "withGpu": false}}], "defaultValue": "large"}],
  "tags": [{"name": "lsmquant", "type": "task"}]
}
