{
  "name": "evexplorer-molecular-evolution",
  "title": "EvExplorer",
  "description": " Molecular Evolution",
  "version": "1.0.0",
  "engineType": "NEXTFLOW",
  "command": "nextflow run nf-core/evexplorer --input ${input} --outdir ${outdir} -r  Evolutionary genomics analysis:1.0.0 ",
  "imageUrl": "https://raw.githubusercontent.com/nf-core/evexplorer/master/docs/images/nf-core-evexplorer_logo_light.png",
  "content": "<h2><strong>EvExplorer</strong></h2><p> Molecular Evolution</p>",
  "spec": [
    {"type": "Stash File", "label": "Input", "name": "input", "required": true, "restrictions": {"allow_files": true, "allowed_file_types": [".csv", ".tsv"]}},
    {"type": "Stash File", "label": "Output", "name": "outdir", "defaultValue": "./evexplorer-results", "required": true, "restrictions": {"allow_files": false, "allow_folders": true}}
  ],
  "jobConfig": [{"type": "Select", "label": "Resources", "name": "system_size", "options": [{"label": "LARGE", "value": "large", "mapValue": {"nodeSize": "LARGE", "numNodes": 1, "withGpu": false}}], "defaultValue": "large"}],
  "tags": [{"name": "evexplorer", "type": "task"}]
}
