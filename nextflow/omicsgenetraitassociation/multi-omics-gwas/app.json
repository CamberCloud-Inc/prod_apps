{
  "name": "omicsgenetraitassociation-multi-omics-gwas",
  "title": "Omics Gene-Trait Association",
  "description": " Multi-omics GWAS",
  "version": "1.0.0",
  "engineType": "NEXTFLOW",
  "command": "nextflow run nf-core/omicsgenetraitassociation --input ${input} --outdir ${outdir} -r  Integrative QTL mapping:1.0.0 ",
  "imageUrl": "https://raw.githubusercontent.com/nf-core/omicsgenetraitassociation/master/docs/images/nf-core-omicsgenetraitassociation_logo_light.png",
  "content": "<h2><strong>Omics Gene-Trait Association</strong></h2><p> Multi-omics GWAS</p>",
  "spec": [
    {"type": "Stash File", "label": "Input", "name": "input", "required": true, "restrictions": {"allow_files": true, "allowed_file_types": [".csv", ".tsv"]}},
    {"type": "Stash File", "label": "Output", "name": "outdir", "defaultValue": "./omicsgenetraitassociation-results", "required": true, "restrictions": {"allow_files": false, "allow_folders": true}}
  ],
  "jobConfig": [{"type": "Select", "label": "Resources", "name": "system_size", "options": [{"label": "LARGE", "value": "large", "mapValue": {"nodeSize": "LARGE", "numNodes": 1, "withGpu": false}}], "defaultValue": "large"}],
  "tags": [{"name": "omicsgenetraitassociation", "type": "task"}]
}
